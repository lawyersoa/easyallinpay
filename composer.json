{
    "name": "lawyersoa/easyallinpay",
    "description": "EasyAllInPay, AllInPay SDK for PHP",
    "type": "library",
    "license": "MIT",
	"keywords": [
		"allinpay",
		"tonglian",
		"easyallinpay",
		"通联"
	],
    "authors": [
        {
            "name": "lawyersoa",
            "email": "business@lawyersoa.com"
        }
    ],
    "require": {
		"php": ">=7.0",
        "overtrue/http": "^1.1.2",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^1.0",
        "monolog/monolog": "^2.2",
        "symfony/cache": "^3.3 || ^4.0 || ^5.0",
        "symfony/http-foundation": "^3.2 || ^4.0 || ^5.0",
        "doctrine/annotations": "^1.12",
        "symfony/serializer": "^5.2",
        "symfony/property-access": "^5.2",
        "ramsey/uuid": "^4.1",
        "doctrine/collections": "^1.6"
	},
	"require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "EasyAllInPay\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyAllInPay\\Tests\\": "tests/"
        }
    }
}
